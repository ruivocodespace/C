//caso_6
#include <stdio.h>

int main() {
    
int opcao;
    // Write C code here
    printf("Digite o número do mês (1 a 12):\n");
    scanf("%i", &opcao);
    
    switch (opcao){
        case 1:
        printf("Janeiro é de alta temporada.");
        break;
        
        case 2:
        printf("Fevereiro é de alta temporada.");
        break;
        
        case 3:
        printf("Março é de baixa temporada.");
        break;
        
        case 4:
        printf("Abril é de baixa temporada.");
        break;
        
        case 5:
        printf("Maio é de baixa temporada.");
        break;
        
        case 6:
        printf("Junho é de alta temporada.");
        break;
        
        case 7:
        printf("Julho é de alta temporada.");
        break;
        
        case 8:
        printf("Agosto é de baixa temporada.");
        break;
        
        case 9:
        printf("Setembro é de baixa temproada.");
        break;
        
        case 10:
        printf("Outubro é de baixa temporada.");
        break;
        
        case 11:
        printf("Novembro é de baixa temporada.");
        break;
        
        case 12:
        printf("Dezembro é de alta temporada.");
        break;
        
        default:
        printf("Inválido");
        break;
    }

    return 0;
}
